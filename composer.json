{
    "name": "delta1186/laravel-connection-migrations",
    "description": "Per-connection migration path configuration for Laravel multi-database applications",
    "keywords": ["laravel", "migrations", "database", "multi-database"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Delta1186\\ConnectionMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delta1186\\ConnectionMigrations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Delta1186\\ConnectionMigrations\\ConnectionMigrationsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
